{
  "name": "wangzhan",
  "version": "1.0.0",
  "description": "Personal website with backend API for student library management",
  "main": "index.html",
  "scripts": {
    "dev": "python3 -m http.server 8080",
    "server:dev": "tsx watch server/src/server.ts",
    "prisma:generate": "prisma generate --schema server/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema server/prisma/schema.prisma",
    "prisma:seed": "tsx server/prisma/seed.ts",
    "setup": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\""
  },
  "keywords": [
    "express",
    "prisma",
    "student-management",
    "library"
  ],
  "author": "田家一",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "@prisma/client": "^5.6.0"
  },
  "devDependencies": {
    "prisma": "^5.6.0",
    "tsx": "^4.6.0",
    "concurrently": "^8.2.2",
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.16",
    "@types/morgan": "^1.9.9",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "typescript": "^5.2.2"
  }
}